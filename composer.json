{
    "name": "alimudasirpcr/laravel-webrtc",
    "description": "A Laravel package for WebRTC integration",
    "type": "library",
    "require": {
        "php": ">=8.0",
        "laravel/framework": "^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "ali\\WebRTC\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ali\\WebRTC\\WebRTCServiceProvider"
            ]
        }
    }
}
